      "XSOURCEShort", ",", "Px1TARGET", ",", "Px0TARGET", ",", "hxTARGET", 
      ",", "Px1SOURCE", ",", "Px0SOURCE", ",", "hxSOURCE", ",", "XXt", ",", 
