     "\[IndentingNewLine]", 
     RowBox[{"XTARGETShort", "=", 
      RowBox[{"Take", "[", 
       RowBox[{"XTARGET", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\[Tau]", "+", "1"}], ",", "T"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"XSOURCEShort", "=", 
